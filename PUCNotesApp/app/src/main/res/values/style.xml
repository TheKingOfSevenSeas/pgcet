<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="sty" parent="">
        <item name="cornerSize">20%</item>
    </style>

    <style name="tabTextAppearance">
        <item name="android:textSize">14dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:fontFamily">@font/abcd</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ToolbarTitleMain">
        <item name="android:textSize">22dp</item>
        <item name="android:textColor">@color/textviewLan</item>
        <item name="android:fontFamily">@font/abcd</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="ToolbarTitlePdf">
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">@color/descPdf</item>
        <item name="android:fontFamily">@font/abcd</item>
        <item name="android:textAlignment">center</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="OverflowMenuTextAppearance">
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/abcd</item>
        <item name="android:textColor">@color/textviewLan</item>
    </style>
</resources>